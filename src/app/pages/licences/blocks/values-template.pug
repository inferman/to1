- var headersList = ["List price", "ERP", "ERP margin", "sales margin", "Fixed $", "effective<br>sales price", "effective<br>sales margin", "MARGIN<br>type", "discount table"]

.settings_values-wrapper
  .settings_values-table
    ul.settings_values_thead.settings_values-container
      each th in headersList
        li.settings_values_th.settings_values-item !{th}

    ul.settings_values_tbody.settings_values-container
      -for(var i = 1; i<=10; i++)
        li.settings_values_td.settings_values-item #{i}


  .settings_values-body
    .row.align-middle
      .columns.xlarge-7.small-6
        .values_taxes-block.row.collapse
          .controls-holder.columns.shrink
            .switcher-wrapper
              .can-toggle.filters-toggle
                input(id="e" type="checkbox")
                label(for="e")
                  .can-toggle__switch(
                  data-checked="Physical",
                  data-unchecked="Digital")
            .select-holder
              simple-select(
                [selectList]="taxesList",
                [selectedItem]="taxesList[0]"
                )
          .columns.shrink
            .select-holder
              simple-select(
                [selectList]="taxesList",
                [selectedItem]="taxesList[0]"
              )
      .columns.xlarge-5.small-6
        .btns-row
          button.history-btn.btn.btn--bordered(type="button")
            .inner-older
              i.btn-icon.fa.fa-history(aria-hidden="true")
              span History
          button.btn.btn--color-filled.btn--secondary(type="button")
            .inner-older
              i.btn-icon.fa.fa-refresh(aria-hidden="true")
              span Reset ERP

          button.btn.btn--color-filled.btn--primary(type="button")
            .inner-older
              i.btn-icon.fa.fa-check(aria-hidden="true")
              span Apply changes




